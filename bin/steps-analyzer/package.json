{
  "name": "steps-analyzer",
  "version": "1.0.0",
  "description": "Identifies unused and duplicate Cucumber step definitions",
  "type": "module",
  "main": "index.js",
  "bin": {
    "steps-analyzer": "./index.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix"
  },
  "keywords": [
    "cucumber",
    "testing",
    "step-definitions",
    "unused-code",
    "duplicates",
    "quality",
    "cleanup"
  ],
  "author": "DEFRA",
  "license": "ISC",
  "dependencies": {
    "glob": "^10.5.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.57.0"
  }
}
